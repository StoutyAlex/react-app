    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-destructuring": "^7.8.8",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
