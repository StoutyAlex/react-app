{
  "extends": "airbnb",
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ],
  "rules": {
      "indent": ["warn", 2],
      "react/jsx-indent": ["warn", 2],
      "react/jsx-props-no-spreading": "off",
      "react/no-array-index-key": "off"
  },
  "env": {
      "jest": true,
      "browser": true
  },
  "settings": {
      "react": {
          "version": "16.13.0"
      }
  }
}
